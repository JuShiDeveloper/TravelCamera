<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--#f4f4f4-->
    <!--#f5f5f5-->
    <!--第一层（最底下）-->
    <item
        android:left="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <solid android:color="#f7f7f7" />
        </shape>
    </item>
    <!--第二层-->
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#f9f9f9" />
        </shape>
    </item>
    <!--第三层-->
    <item
        android:bottom="3dp"
        android:left="2.5dp"
        android:right="2.5dp"
        android:top="1.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#ffffff" />
        </shape>
    </item>

</layer-list>